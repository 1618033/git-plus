{
  "name": "git-plus",
  "main": "./lib/git-plus",
  "version": "4.4.13",
  "description": "Do git things without the terminal",
  "keywords": [
    "git"
  ],
  "activationCommands": {
    "atom-workspace": [
      "git-plus:menu",
      "git-plus:add",
      "git-plus:add-all",
      "git-plus:commit",
      "git-plus:commit-amend",
      "git-plus:add-and-commit",
      "git-plus:add-all-and-commit",
      "git-plus:add-all-commit-and-push",
      "git-plus:checkout-current-file",
      "git-plus:checkout",
      "git-plus:diff",
      "git-plus:diff-all",
      "git-plus:remove",
      "git-plus:remove-current-file",
      "git-plus:show",
      "git-plus:log"
    ]
  },
  "repository": "https://github.com/akonwi/git-plus",
  "license": "MIT",
  "engines": {
    "atom": ">0.50.0"
  },
  "dependencies": {
    "fs-plus": "^2.2.0",
    "fuzzaldrin": "^1.2.0",
    "underscore-plus": "1.x",
    "atom-space-pen-views": "^2.0.3"
  }
}
