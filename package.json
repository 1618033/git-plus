{
  "name": "git-plus",
  "main": "./lib/git-plus",
  "version": "2.7.3",
  "description": "Do git things without the terminal",
  "activationEvents": [
    "git-plus:commit",
    "git-plus:add",
    "git-plus:add-all",
    "git-plus:checkout-current-file",
    "git-plus:commit-amend",
    "git-plus:diff",
    "git-plus:diff-all",
    "git-plus:checkout",
    "git-plus:new-branch",
    "git-plus:pull",
    "git-plus:push",
    "git-plus:fetch",
    "git-plus:add-and-commit",
    "git-plus:add-all-and-commit",
    "git-plus:remove",
    "git-plus:remove-current-file",
    "git-plus:log",
    "git-plus:log-current-file",
    "git-plus:show",
    "git-plus:tags"
  ],
  "repository": "https://github.com/akonwi/git-plus",
  "license": "MIT",
  "engines": {
    "atom": ">0.50.0"
  },
  "dependencies": {
    "fs-plus": "^2.2.0"
  }
}
